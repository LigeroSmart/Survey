                              <td rowspan="4">&nbsp;&nbsp;&nbsp;<input class="button" type="submit" value="$Text{"Add"}"></td>
                            </tr>
                            <tr>
                              <td class="contentvalue"><input type="radio" name="QuestionType" value="2">&nbsp;(2)&nbsp;$Text{"List"}&nbsp;-&nbsp;$Text{"Radio"}</td>
                            </tr>
                            <tr>
                              <td class="contentvalue"><input type="radio" name="QuestionType" value="3">&nbsp;(3)&nbsp;$Text{"List"}&nbsp;-&nbsp;$Text{"Checkbox"}</td>
                            </tr>
                            <tr>
                              <td class="contentvalue"><input type="radio" name="QuestionType" value="4">&nbsp;(4)&nbsp;$Text{"Textarea"}</td>
                            </tr>
                          </table>
                        </form>
                      </td>
                      <td class="contentkey" width="25">&nbsp;</td>
                    </tr>
                    <tr>
                      <td class="contentkey" width="25">&nbsp;</td>
                      <td class="contentkey">&nbsp;</td>
                      <td class="contentkey" width="25">&nbsp;</td>
                    </tr>
<!-- dtl:block:SurveyEditNewQuestion -->
<!-- dtl:block:SurveyEditQuestionTypes -->
