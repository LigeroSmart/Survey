<!-- dtl:block:Overview -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Overview"}: $Text{"Survey"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="mainbody" align="center">
      <br>
      <table border="0" width="800" cellspacing="0" cellpadding="3">
        <tr class="contenthead">
          <td class="contentkey" width="3%">&nbsp;</td>
          <td class="contentkey">$Text{"Number"}</td>
          <td class="contentkey">$Text{"Title"}</td>
          <td class="contentkey">$Text{"Status"}</td>
          <td class="contentkey" width="18%">$Text{"Created"}</td>
        </tr>
<!-- dtl:block:OverviewSurvey -->
        <dtl if ($Env{"Color"} eq "") { $Env{"Color"} = "searchpassive"; }>
        <tr class="contentbody">
          <td class="$Env{"Color"}">&nbsp;</td>
          <td class="$Env{"Color"}"><a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=Survey&SurveyID=$QData{"SurveyID"}">$QData{"SurveyNumber"}</a></td>
          <td class="$Env{"Color"}">$QData{"SurveyTitle","60"}</td>
          <td class="$Env{"Color"}">$Text{"$QData{"SurveyStatus"}"}</td>
          <td class="$Env{"Color"}">$TimeLong{"$QData{"SurveyCreateTime"}"}</td>
        </tr>
        <dtl if ($Env{"Color"} eq "searchactive") { $Env{"Color"} = ""; }>
        <dtl if ($Env{"Color"} eq "searchpassive") { $Env{"Color"} = "searchactive"; }>
<!-- dtl:block:OverviewSurvey -->
        <tr class="contentfooter">
          <td colspan="5">&nbsp;</td>
        </tr>
      </table>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:Overview -->

