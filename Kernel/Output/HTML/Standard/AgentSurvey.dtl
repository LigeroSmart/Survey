<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Overview"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="mainbody">

      <br>

      <form action="$Env{"CGIHandle"}" method="post" enctype="multipart/form-data" name="compose">
        <input type="hidden" name="Action" value="$Env{"Action"}">
        <input type="hidden" name="Subaction" value="SurveyAdd">

        <table border="0" width="700" align="center" cellspacing="0" cellpadding="4">
          <tr>
            <td colspan="4" class="contenthead">$Text{"Surveys"}</td>
          </tr>
          <tr>
            <td class="contentbody">
              <table border="0" width="100%">
                <tr>
                  <td class="contentkey" width="25">&nbsp;</td>
                  <td class="contentkey" width="80">&nbsp;</td>
                  <td class="contentkey">&nbsp;</td>
                  <td class="contentkey" width="60">&nbsp;</td>
                  <td class="contentkey" width="25">&nbsp;</td>
                </tr>
                <tr>
                  <td class="contentkey" width="25">&nbsp;</td>
                  <td class="contentkey" width="80">$Text{"Number"}</td>
                  <td class="contentkey">$Text{"Title"}</td>
                  <td class="contentkey" width="60">$Text{"Status"}</td>
                  <td class="contentkey" width="25">&nbsp;</td>
                </tr>
                <tr>
                  <td class="contentkey" width="25">&nbsp;</td>
                  <td class="contentkey" width="80">&nbsp;</td>
                  <td class="contentkey">&nbsp;</td>
                  <td class="contentkey" width="60">&nbsp;</td>
                  <td class="contentkey" width="25">&nbsp;</td>
                </tr>
<!-- dtl:block:OverviewSurvey -->
                <dtl set $Data{"State"} = "valid">
                <dtl if ($Data{"SurveyMaster"} eq "Yes") { $Data{"State"} = "master"; }>
                <dtl if ($Data{"SurveyValid"} ne "Yes") { $Data{"State"} = "novalid"; }>

                <tr>
                  <td class="contentkey" width="25">&nbsp;</td>
                  <td class="$Data{"State"}" width="80"><a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=Survey&SurveyID=$QData{"SurveyID"}">$QData{"SurveyNumber"}</a></td>
                  <td class="$Data{"State"}">$QData{"SurveyTitle"}</td>
                  <td class="$Data{"State"}" width="60">$Text{"$QData{"SurveyStatus"}"}</td>
                  <td class="contentkey" width="25">&nbsp;</td>
                </tr>
<!-- dtl:block:OverviewSurvey -->
                <tr>
                  <td class="contentkey" width="25">&nbsp;</td>
                  <td class="contentkey" width="80">&nbsp;</td>
                  <td class="contentkey">&nbsp;</td>
                  <td class="contentkey" width="60">&nbsp;</td>
                  <td class="contentkey" width="25">&nbsp;</td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td class="contentfooter">&nbsp;
            </td>
          </tr>
        </table>
      </form>

      <br>

    </td>
  </tr>
</table>
<!-- dtl:block:Overview -->

